        <activity android:name=".Activities.VersusActivity">
        </activity>
        <activity android:name=".Activities.NextStepActivity">
        </activity>
        <activity android:name=".Activities.DownloadImageActivity">
        </activity>
        <activity android:name=".Activities.HomeActivity">
